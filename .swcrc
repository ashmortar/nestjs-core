{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true,
      "dynamicImport": true
    },
    "baseUrl": "./",
    "paths": {
      "@core/*": ["./src/*"],
      "@generated/*": ["./generated/*"]
    },
    "transform": {
      "react": {
        "importSource": "@kitajs/html",
        "runtime": "automatic"
      }
    }
  },
  "minify": false
}

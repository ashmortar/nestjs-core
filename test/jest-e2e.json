{
  "moduleFileExtensions": ["js", "json", "ts", "jsx", "tsx"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest",
    "^.+\\.(t|j)sx$": "ts-jest"
  },
  "moduleNameMapper": {
    "@core/(.*)": "<rootDir>/$1",
    "@generated/(.*)": "<rootDir>/generated/$1"
  }
}
